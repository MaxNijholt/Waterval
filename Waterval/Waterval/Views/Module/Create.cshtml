    <div class="form-group">
        <div class="container">
            <p>

                @Html.Label("Weekplanning Toevoegen", new { @class = "col-sm-2 control-label" })
            </p>
            <div id="ValidationMes" class="alert alert-danger hide">
                Minstens een Weekplanning verplicht

            </div>
            <br /><br />
            <table class="table table-bordered table-striped table-responsive" id="LongTable">
                <thead>
                    <tr>
                        <th width="5%">Weeknr</th>
                        <th width="80%">Onderwerp</th>
                        <th>Verwijderen</th>
                    </tr>
                </thead>
             

            </table>
            <input type="button" value="Weekplanning Toevoegen" name="btn_collapseGoal" class="btn btn-avans" data-toggle="collapse" data-target="#collapseMod">


            <br />
            <br />
            <div id="collapseMod" class="panel-collapse collapse open">
                <form action="" class="form-inline">
                    <table class="table">
                        <tr>
                            <td width="10%">
                                <input type="text" class="form-control" placeholder="#" name="TempValue" style:size="10" />
                            </td>
                            <td width="50%">
                                <input type="text" class="form-control" placeholder="weekbeschrijving..." name="TempValue" style:size="10" />
                            </td>
                            <td width="10%">
                                <input type="button" value="Toevoegen" name="addGoal" class="btn btn-avans">
                            </td>

                            <td width="10%">
                                <input type="button" value="Toevoegen Na" name="addGoalBetween" class="btn btn-avans">
                            </td>
                            <td width="20%">
                                <select id="selecteer" class="form-control"><option value="-1" disabled selected>Maak een keuze</option></select>
                            </td>
                        </tr>
                    </table>
                </form>
            </div>
        </div>
    </div>

    <hr />



