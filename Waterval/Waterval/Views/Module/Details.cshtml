
@model DomainModel.Models.Module

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Styles.Render("~/Content/css")
<div class="container">
    <h2>@Html.DisplayFor(model => model.Title)</h2>
    <fieldset>
        <table class="table table-striped table-bordered">
            <tr>
                <td>
                    @Html.DisplayNameFor(model => model.Title)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Title)
                </td>
            </tr>
            <tr>
                <td>
                    @Html.DisplayNameFor(model => model.Definition_Short)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Definition_Short)
                </td>
            </tr>
            <tr>
                <td>
                    @Html.DisplayNameFor(model => model.Definition_Long)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Definition_Long)
                </td>
            </tr>
            <tr>
                <td>
                    @Html.DisplayNameFor(model => model.CourseCode)
                </td>
                <td>
                    @Html.DisplayFor(model => model.CourseCode)
                </td>
            </tr>
            <tr>
                <td>
                    @Html.DisplayNameFor(model => model.Account_ID)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Account_ID)
                </td>
            </tr>
            <tr>
                <td>
                    @Html.DisplayNameFor(model => model.Foreknowledge)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Foreknowledge)
                </td>
            </tr>
            <tr>
                <td>
                    @Html.DisplayNameFor(model => model.Entry_Level)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Entry_Level)
                </td>
            </tr>
        </table>


        <div class="container">
            <p>Leerdoelen</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Doel</th>
                </tr>
                <tr>
                    <td>in een praktijksituatie een keuze maken tussen het gebruik van een gekoppelde lijst, array, binaire boom en een hashtabel.</td>
                </tr>
            </table>
        </div>


        <div class="container">
            <p>Leermiddelen</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Leermiddel</th>
                </tr>
                <tr>
                    <td>Boek: Data structures & algorithms in JAVA, Second edition, Robert Lafore, ISBN: 0 672 32453 9</td>
                </tr>
            </table>
        </div>

        <div class="container">
            <p>Weekplanning</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Weeknr</th>
                    <th>Onderwerp</th>
                </tr>
                <tr>
                    <td>1</td>
                    <td>Introductie</td>
                </tr>
                <tr>
                    <td>2</td>
                    <td>Variables, Methods.</td>
                </tr>
            </table>
        </div>


        <div class="container">
            <p>Beoordelingen</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Onderwerp</th>
                </tr>
                <tr>
                    <td>Assesment</td>
                </tr>
            </table>
        </div>


        <div class="container">
            <p>Toetscode</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Omschrijving</th>
                    <th>EC</th>
                </tr>
                <tr>
                    <td>Prog1-tentamen</td>
                    <td>4</td>
                </tr>
            </table>
        </div>

        <div class="container">
            <p>Werkvorm</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Werkvorm</th>
                    <th>Duur</th>
                    <th>Studiebelasting</th>
                    <th>Frequentie</th>
                </tr>
                <tr>
                    <td>Hoorcollege</td>
                    <td>52 uur</td>
                    <td>1</td>
                    <td>2x2 weken</td>
                </tr>
                <tr>
                    <td>Practicum</td>
                    <td>104 uur</td>
                    <td>2</td>
                    <td>2x4 weken</td>
                </tr>
            </table>
        </div>


        <div class="container">
            <p>Leerlijnen</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Naam</th>
                    <th>Omschrijving</th>
                </tr>
                <tr>
                    <td>@Html.ActionLink("Programeren", "Details", "Leerlijn")</td>
                    <td>Het leren van programeren</td>
                </tr>
                <tr>
                    <td>@Html.ActionLink("Onderzoek", "Details", "Leerlijn")</td>
                    <td>Het leren van Onderzoeken</td>
                </tr>
            </table>
        </div>


        <div class="container">
            <p>Opleiding</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Cohort</th>
                </tr>
                <tr>
                    <td>@Html.ActionLink("Voorbeeld", "Details", "Leerlijn")</td>
                </tr>
            </table>
        </div>

        <div class="container">
            <p>Studierichting</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Onderwerp</th>
                </tr>
                <tr>
                    <td>@Html.ActionLink("SO", "Details", "Leerlijn")</td>
                </tr>
            </table>
        </div>


        <div class="container">
            <p>Thema's</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Naam</th>
                    <th>Omschrijving</th>
                </tr>
                <tr>
                    <td>@Html.ActionLink("Programeren", "Details", "Thema")</td>
                    <td>Het leren van programeren</td>
                </tr>
                <tr>
                    <td>@Html.ActionLink("Onderzoek", "Details", "Thema")</td>
                    <td>Het leren van Onderzoeken</td>
                </tr>
            </table>

        </div>

        <div class="container">
            <p>Compententie's</p>
            <table class="table table-bordered table-striped">
                <tr>
                    <th>Titel</th>
                    <th>Omschrijving</th>
                </tr>
                <tr>
                    <td>@Html.ActionLink("Procesanalyse uitvoeren", "Details", "Compententie")</td>
                    <td>De student is in staat om een procesanalyse uit te voeren.</td>
                </tr>
            </table>

        </div>

    </fieldset>

    <p>
      @Html.ActionLink("Aanpassen", "Edit", null, new { @class = "btn btn-warning" })
        <br><input type="submit" class="btn btn-danger" name="Delete" value="Verwijderen" /><br />|
        @Html.ActionLink("Terug naar Index", "Index")
    </p>
</div>
