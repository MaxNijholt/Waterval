@model DomainModel.Models.Module
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("toNewVersion", "Module", new { ReturnUrl = ViewBag.ReturnUrl },
            FormMethod.Post, new { @class = "form-horizontal", @role = "form" }))
{

    @Html.ValidationSummary(true)


    <fieldset>
        <legend></legend>
        @Html.HiddenFor(model => model.PrevModule_ID)
        @Html.Partial("_CreateOrEdit", Model)
        <p>
            <input type="submit" value="Module opslaan" class="btn btn-success" /> |
            @Html.ActionLink("Terug naar Index", "Index")
        </p>
    </fieldset>
}


@section Scripts {

    @Scripts.Render("~/bundles/jqueryval")
        <script src="~/Scripts/ScriptModule.js"></script>
}


