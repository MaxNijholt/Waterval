@model IEnumerable<DomainModel.Models.Block>

@{
    ViewBag.Title = "BlockResult";
}

<h2>BlockResult</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.ActionLink("Details", "../Block/Details", new {  id=item.Block_ID })
        </td>
    </tr>
}

</table>
